#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(int argc, char *argv[]) {
    
    if (argc < 2) {
       printf("Need another arg");
    }
    int num = atoi(argv[1]);
    for (int i = 0; i < num; i++) {
    	printf("Show me da wey");
    }
}

int lcg(int m, int seed) {

    return 2.5 * seed + 3 (% m);
}